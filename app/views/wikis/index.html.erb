<h1>All Wikis</h1>
<% @wikis.each do |wiki| %>
  <% if current_user.admin? || current_user.premium? %>
    <p><%=link_to wiki.title, wiki_path(wiki.id) %></p>
  <% else %>
    <% if !wiki.private %>
      <p><%=link_to wiki.title, wiki_path(wiki.id) %></p>
    <% end %>
  <% end %>
<% end %>

<div class="col-md-4">
  <%= link_to "New Wiki", new_wiki_path(@wiki), class: 'btn btn-success' %>
</div>
