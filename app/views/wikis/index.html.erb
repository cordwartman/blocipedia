<h1>All Wikis</h1>
<div class="col-md-4">
  <%= link_to "New Wiki", new_wiki_path(@wiki), class: 'btn btn-success' %>
</div>
</br>
</br>
<div>
  <% @wikis.each do |wiki| %>
    <p><%=link_to wiki.title, wiki_path(wiki.id) %> <% if wiki.private? %>   <b>| Private</b> <% end %></p>
  <% end %>
</div>

